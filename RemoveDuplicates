import java.util.*;

public class RotateArray {

    public static void main(String[] args) {
        String s = "geeksforgeeks";
        String result="";
        char[]c=s.toCharArray();
        Arrays.sort(c);
        result+=c[0];
        for(int i=1;i<s.length();i++)
       {
          if(c[i]!=c[i-1])
              result+=c[i];
       }
       System.out.println(result);
}}
