import java.util.Arrays;

public class HighestMultiplication {


    public static void main(String[]args)
    {
        int[]arr={0,-1,3,100,70,50};
        int len=arr.length;
        if(arr.length<3)
        {
            System.out.println("less than 3 numbers");
            System.exit(0);
        }
        Arrays.sort(arr);
        int mul=arr[len-3]*arr[len-2]*arr[len-1];
        System.out.println("max mul is "+mul);
    }
}
